<scenefile>
    <globaldata>
        <diffusecoeff v="0.5"/>
        <specularcoeff v="0.5"/>
        <ambientcoeff v="0.5"/>
    </globaldata>

    <!-- from left ->
    <cameradata>
        <pos x="5" y="2.5" z="0"/>
        <up x="0" y="1" z="0"/>
        <focus x="-10" y="-1" z="-1"/>
        <heightangle v="40"/>
    </cameradata>
    <-->
    <!-- front -->
    <cameradata>
        <pos x="0" y="2" z="5"/>
        <up x="0" y="1" z="0"/>
        <focus x="0" y="0" z="-10"/>
        <heightangle v="40"/>
    </cameradata>
    <-->
    <!-- top ->
    <cameradata>
        <pos x="0" y="5" z="1.15"/>
        <up x="0" y="1" z="0"/>
        <focus x="-0.03" y="-10" z="0"/>
        <heightangle v="40"/>
    </cameradata>
    <-->

    <lightdata>
    <id v="0"/>
    <color r="1" g="1" b="1"/>
    <position x="4" y="4" z="4"/>
    </lightdata>

    <lightdata>
    <id v="1"/>
    <color r="1" g="1" b="1"/>
    <position x="-3" y="3" z="3"/>
    </lightdata>

    <lightdata>
    <id v="2"/>
    <color r="1" g="1" b="1"/>
    <position x="-3" y="3" z="-3"/>
    </lightdata>

    <lightdata>
    <id v="3"/>
    <color r="1" g="1" b="1"/>
    <position x="3" y="3" z="-3"/>
    </lightdata>


    <object type="tree" name="root">
    <transblock> <!--head -->
      <translate x="0" y="2" z="0"/>
      <object type="tree"> 
      <transblock>
        <object type="primitive" name="sphere" >
        <diffuse r="0" g="1" b="0"/>
        </object>
      </transblock>
      <transblock>
        <translate x="-.45" y=".4" z="0" />
        <rotate x="0" y="0" z="1" angle="45" />
        <scale x=".25" y=".25" z=".25" />
        <object type="primitive" name="cone">
        <diffuse r="0" g="1" b="0"/>
        </object>
      </transblock>
      <transblock>
        <translate x=".45" y=".4" z="0" />
        <rotate x="0" y="0" z="1" angle="-40" />
        <scale x=".25" y=".35" z=".25" />
        <object type="primitive" name="cone">
        <diffuse r="0" g="1" b="0"/>
        </object>
      </transblock>
      <transblock>
        <translate x=".25" y=".1" z=".4" />
        <rotate x="0" y="0" z="0" angle="0" />
        <scale x=".15" y=".15" z=".15" />
        <object type="primitive" name="sphere">
        <diffuse r="0" g="0" b="1"/>
        </object>
      </transblock>
      <transblock>
        <translate x="-.25" y=".1" z=".4" />
        <rotate x="0" y="0" z="0" angle="0" />
        <scale x=".15" y=".15" z=".15" />
        <object type="primitive" name="sphere">
        <diffuse r="0" g="0" b="1"/>
        </object>
      </transblock>
      <transblock>
        <translate x="0" y="-.1" z=".5" />
        <rotate x="0" y="0" z="1" angle="180" />
        <scale x=".15" y=".15" z=".15" />
        <object type="primitive" name="cone">
        <diffuse r="0" g="0" b="1"/>
        </object>
      </transblock>
      </object>
    </transblock>
    <transblock> <!-- hindquarters -->
      <object type="tree">
        <transblock>
        <translate x="0" y=".45" z="-.4" />
        <scale x="1.15" y="1" z="1" />
        <object type="primitive" name="sphere">
        <diffuse r="1" g="0" b="0"/>
        </object>
        </transblock>
        <transblock>
        <translate x="0" y="0" z="-.65" />
        <rotate x="1" y="0" z="0" angle="90" />
        <rotate x="0" y="1" z="0" angle="35" />
        <scale x=".15" y=".85" z=".15" />
        <object type="primitive" name="cylinder">
        <diffuse r="0" g="0" b="1"/>
        </object>
        </transblock>
        <transblock>
        <translate x=".2" y=".2" z="-1.35" />
        <rotate x="1" y="1" z="0" angle="-90" />
        <scale x=".15" y=".85" z=".15" />
        <object type="primitive" name="cylinder">
        <diffuse r="0" g="0" b="1"/>
        </object>
        </transblock>
      </object>
    </transblock>
    <transblock> <!-- body -->
      <translate x="0" y="1" z="-.25" />
      <object type="tree"> 
        <transblock>
          <rotate x="1" y="0" z="0" angle="25" />
          <scale x="1" y="1.75" z="1" />
          <object type="primitive" name="sphere">
          <diffuse r="1" g="0" b="0"/>
          </object>
        </transblock>
        <transblock>
          <translate x=".13" y="-.25" z=".45" />
          <scale x=".25" y="1.25" z=".25" />
          <object type="primitive" name="cylinder"> 
          <diffuse r="1" g="0" b="0"/>
          </object>
        </transblock>
        <transblock>
          <translate x="-.13" y="-.25" z=".45" />
          <scale x=".25" y="1.25" z=".25" />
          <object type="primitive" name="cylinder"> 
          <diffuse r="1" g="0" b="0"/>
          </object>
        </transblock>
      </object>
    </transblock>
    <transblock> <!-- feet -->
      <scale x=".3" y=".2" z=".3" />
      <translate x="0" y=".25" z="1" />
      <object type="tree">
        <transblock>
        <translate x=".55" y="0" z="0" />
        <object type="primitive" name="sphere">
        <diffuse r="1" g="1" b="0"/>
        </object>
        </transblock>
        <transblock>
        <translate x="-.55" y="0" z="0" />
        <object type="primitive" name="sphere">
        <diffuse r="1" g="1" b="0"/>
        </object>
        </transblock>
        <transblock>
        <translate x="1.5" y=".15" z="-1.75" />
        <object type="primitive" name="sphere">
        <diffuse r="1" g="1" b="0"/>
        </object>
        </transblock>
        <transblock>
        <translate x="-1.5" y=".15" z="-1.75" />
        <object type="primitive" name="sphere">
        <diffuse r="1" g="1" b="0"/>
        </object>
        </transblock>
      </object>
    </transblock>
    </object>



</scenefile>

