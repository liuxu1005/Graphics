<scenefile>
	<globaldata>
		<diffusecoeff v="0.5"/>
		<specularcoeff v="0.5"/>
		<ambientcoeff v="0.5"/>
	</globaldata>

	<cameradata>
		<pos x="0" y="0" z="6"/>
		<up x="0" y="1" z="0"/>
		<focus x="0" y="0" z="0"/>
		<heightangle v="40"/>
	</cameradata>

	<lightdata>
		<id v="0"/>
		<color r="1" g="1" b="1"/>
		<function v1="1" v2="0" v3="0"/>
		<position x="0" y="0" z="4"/>
	</lightdata>

	<object type="tree" name="aCube">
		<transblock>
			<object type="primitive" name="cube" >
				<diffuse r="0.3" g="0.3" b="0.3"/>
				<ambient r="0.2" g="0.2" b="0.2"/>
			</object>
		</transblock>
	</object>

	<object type="tree" name="root">
		<transblock>
			<translate x="-1.2" y="0.6" z="0"/>
			<rotate x="0" y="1" z="0" angle="0"/>
			<object type="master" name="aCube" />
		</transblock>
		<transblock>
			<translate x="0" y="0.6" z="0"/>
			<rotate x="0" y="1" z="0" angle="90"/>
			<object type="master" name="aCube" />
		</transblock>
		<transblock>
			<translate x="1.2" y="0.6" z="0"/>
			<rotate x="0" y="1" z="0" angle="180"/>
			<object type="master" name="aCube" />
		</transblock>
		<transblock>
			<translate x="-1.2" y="-0.6" z="0"/>
			<rotate x="0" y="1" z="0" angle="270"/>
			<object type="master" name="aCube" />
		</transblock>
		<transblock>
			<translate x="0" y="-0.6" z="0"/>
			<rotate x="1" y="0" z="0" angle="90"/>
			<object type="master" name="aCube" />
		</transblock>
		<transblock>
			<translate x="1.2" y="-0.6" z="0"/>
			<rotate x="1" y="0" z="0" angle="-90"/>
			<object type="master" name="aCube" />
		</transblock>
	</object>

</scenefile>

